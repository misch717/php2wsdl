{
  "name": "misch717/php2wsdl",
  "description": "Create WSDL files from PHP classes.",
  "type": "library",
  "keywords": [
    "wsdl",
    "wsdl generator",
    "web services"
  ],
  "config": {
    "preferred-install": "dist"
  },
  "homepage": "https://github.com/misch717/phpToWsdl",
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Schlegel",
      "email": "twokay717@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "wingu/reflection": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7",
    "scrutinizer/ocular": "~1.1"
  },
  "autoload": {
    "psr-4": {
      "PHP22WSDL\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PHP2WSDL\\Test\\": "tests"
    }
  }
}
